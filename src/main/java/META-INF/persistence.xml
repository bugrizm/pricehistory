<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="price_retriever">
	    <provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>com.price.history.model.Store</class>
		<class>com.price.history.model.Product</class>
		<class>com.price.history.model.ProductCategory</class>
		<class>com.price.history.model.ProductPrice</class>
		<properties>
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="16061991" />
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.GoogleDriver" />
			<property name="javax.persistence.jdbc.url" value="jdbc:google:mysql://pricehistory-1095:pricehistory-sql/product_prices" />
			<property name="hibernate.show_sql" value="true" />
			<property name="hibernate.hbm2ddl.auto" value="update" />
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQL5InnoDBDialect" />
		</properties>
	</persistence-unit>
	
